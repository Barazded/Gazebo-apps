<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="News_aggregator.SettingPage">
    <ContentPage.Content>
        <StackLayout x:Name="sl_listproperties" BackgroundColor="#212121" Padding="0,20">

            <Button x:Name="settingButt" Clicked="AverageButt_Clicked" Text="Settings" TextColor="#E0E0E0"  FontSize="15" Padding="10,10,10,10"
                FontAttributes="Bold" BackgroundColor="#424242" CornerRadius="2">
            </Button>

            <Button x:Name="profileButt" Clicked="AverageButt_Clicked" Text="Profile" TextColor="#E0E0E0"  FontSize="15" Padding="10,10,10,10"
                FontAttributes="Bold" BackgroundColor="#424242" CornerRadius="2">
            </Button>

            <Button x:Name="resourseButt" Clicked="AverageButt_Clicked" Text="Resourse" TextColor="#E0E0E0"  FontSize="15" Padding="10,10,10,10"
                FontAttributes="Bold" BackgroundColor="#424242" CornerRadius="2" >
            </Button>

        </StackLayout>

    </ContentPage.Content>
</ContentPage>
